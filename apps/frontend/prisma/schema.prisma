generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "mysql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Video {
    id String @id @default(uuid())
    name String
    url String
    prompt String
    reaction String
    demoType String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    
    @@index([reaction])
    @@index([demoType])
    @@index([createdAt])
}